{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "php:serve": "php artisan serve",
        "php:pail": "php artisan pail",
        "php:queue": "php artisan queue:listen --timeout=3600",
        "php:queue-high": "php artisan queue:listen --queue=high",
        "php:pulse": "php artisan pulse:check",
        "php:scheduler": "php artisan schedule:work",
        "php:reverb": "php artisan reverb:start ",
        "php:reverb-debug": "php artisan reverb:start --debug",
        "vite:dev": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold\" \"npm:php:serve\" \"npm:php:queue\" \"npm:dev\"",
        "vite:dev2": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold\" \"npm:php:serve\" \"npm:php:pail\" \"npm:dev\"",
        "vite:cron": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold,white.bold\" \"npm:php:queue\" \"npm:php:scheduler\" \"npm:dev\" \"npm:php:pulse\"",
        "vite:cronws": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold,white.bold,magenta.bold,yellow.bold\" \"npm:php:queue\" \"npm:php:scheduler\" \"npm:dev\" \"npm:php:pulse\" \"npm:php:reverb\" \"npm:php:queue-high\"",
        "vite:cronwsd": "concurrently -k -p \"[{name}]\" -c \"blue.bold,green.bold,red.bold,white.bold,magenta.bold,yellow.bold\" \"npm:php:queue\" \"npm:php:scheduler\" \"npm:dev\" \"npm:php:pulse\" \"npm:php:reverb-debug\" \"npm:php:queue-high\"",
        "vite:php": "concurrently -k -p \"[{name}]\" -c \"blue.bold,white.bold,magenta.bold,yellow.bold\" \"npm:php:queue\" \"npm:php:pulse\" \"npm:php:reverb\" \"npm:php:queue-high\"",
        "vite:phpwsd": "concurrently -k -p \"[{name}]\" -c \"blue.bold,white.bold,magenta.bold,yellow.bold\" \"npm:php:queue\" \"npm:php:pulse\" \"npm:php:reverb-debug\" \"npm:php:queue-high\"",
        "build": "npm run pint && npm run type-check && npm run build-only",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"{resources/js/**/*.{js,ts,vue,json},*.{json,js,ts,md,php}}\"",
        "typegen": "laravel-typegen",
        "pint": "php ./vendor/bin/pint",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "docker:build": "docker build . -t aminnausin/mediaserver:latest && docker push aminnausin/mediaserver:latest",
        "docker:build-beta": "docker build . -t aminnausin/mediaserver:beta && docker push aminnausin/mediaserver:beta"
    },
    "devDependencies": {
        "@7nohe/laravel-typegen": "^0.6.1",
        "@eslint/js": "^9.7.0",
        "@faker-js/faker": "^9.8.0",
        "@iconify-json/cil": "^1.2.2",
        "@iconify-json/circum": "^1.1.11",
        "@iconify-json/formkit": "^1.2.2",
        "@iconify-json/lucide": "^1.1.202",
        "@iconify-json/mage": "^1.2.2",
        "@iconify-json/majesticons": "^1.2.1",
        "@iconify-json/material-symbols-light": "^1.1.25",
        "@iconify-json/mdi-light": "^1.2.1",
        "@iconify-json/ph": "^1.1.13",
        "@iconify-json/prime": "^1.2.2",
        "@iconify-json/proicons": "^1.2.9",
        "@iconify-json/svg-spinners": "^1.2.2",
        "@iconify-json/tabler": "^1.2.17",
        "@iconify-json/uit": "^1.2.3",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tsconfig/node22": "^22.0.1",
        "@types/jsdom": "^21.1.7",
        "@types/lodash.clonedeep": "^4.5.9",
        "@types/lodash.isequal": "^4.5.8",
        "@types/node": "^22.9.3",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.13.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vitest/coverage-v8": "^3.1.4",
        "@vue/eslint-config-prettier": "^10.1.0",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.20",
        "axios": "^1.9.0",
        "concurrently": "^9.1.0",
        "cssnano": "^7.0.6",
        "eslint": "^9.22.0",
        "eslint-plugin-vue": "~10.0.0",
        "jiti": "^2.4.2",
        "jsdom": "^26.1.0",
        "laravel-echo": "^1.19.0",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.47",
        "prettier": "3.5.3",
        "pusher-js": "^8.4.0-rc2",
        "tailwind-scrollbar-hide": "^1.1.7",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.8.0",
        "unplugin-icons": "^22.1.0",
        "vite": "^6.2.6",
        "vite-plugin-compression": "^0.5.1",
        "vitest": "^3.1.4",
        "vue-tsc": "^2.2.8"
    },
    "dependencies": {
        "@tanstack/vue-query": "^5.77.2",
        "@types/lodash": "^4.17.13",
        "@vueuse/components": "^10.11.0",
        "@vueuse/core": "^10.11.0",
        "@vueuse/integrations": "^11.0.0",
        "chart.js": "^4.4.7",
        "focus-trap": "^7.5.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isequal": "^4.5.0",
        "pinia": "^3.0.1",
        "puppeteer": "^24.7.2",
        "vue": "^3.5.13",
        "vue-chartjs": "^5.3.2",
        "vue-router": "^4.5.0"
    }
}
